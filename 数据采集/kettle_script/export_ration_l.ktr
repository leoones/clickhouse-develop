    <connection>oracle_JVDMS</connection>
    <sql>SELECT  to_char(t.outexedate,'yyyy-mm-dd') as deliver_date,
        t.sheetid,
        case when t.rationtype = 'I' then t.outshopid else t.inshopid end outshopid,
        case when t.rationtype = 'I' then t.inshopid else t.outshopid end inshopid,
        replace(t.editor, '\', '') as editor,
        replace(t.operator, '\', '') as operator,
        t2.paytypeid,
        t2.goodsid,
        t2.cost,
        t1.logistics,
        t2.venderid,
        t2.costtaxrate,
        t2.saletaxrate,
        case when t.rationtype = 'I' then 1 else 6  end bun_type,
        t2.outqty rt_qty,
        t2.cost * t2.outqty rt_cost,
         round(t2.cost * t2.outqty / (1 + t2.costtaxrate / 100), 2) rt_costvalue
  FROM DBUSRJVDMS.ration     t,
       DBUSRJVDMS.rationitem t1,
       DBUSRJVDMS.rationacc  t2
 WHERE t.sheetid = t1.sheetid
   AND t.sheetid = t2.sheetid
   AND t1.goodsid = t2.goodsid

   AND t.outexedate >= date'${V_START_DATE}'
   AND t.outexedate  &lt; date'${V_END_DATE}'
   AND t.outshopid  in (
      '995045',
      '995088',
      '995065',
      '995066',
      '995068',
      '995069',
      '995067',
      '995062',
      '995063',
      '995061',
      '995121',
      '995107',
      '995106'
     )
   AND t.rationtype in ( 'I', 'O')
           

                      
 

          

  

</sql>
